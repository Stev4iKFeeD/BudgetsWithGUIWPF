<Window x:Class="Budgets.GUI.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:Budgets.GUI.WPF"
        xmlns:authentication="clr-namespace:Budgets.GUI.WPF.Authentication"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" MinHeight="580" MinWidth="675" FontSize="18">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" MinHeight="110"></RowDefinition>
            <RowDefinition Height="2*"></RowDefinition>
        </Grid.RowDefinitions>

        <wpf:Header></wpf:Header>

        <wpf:MainView Grid.Row="1"></wpf:MainView>
    </Grid>
</Window>
